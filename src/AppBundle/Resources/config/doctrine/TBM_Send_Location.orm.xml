<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="AppBundle\Entity\TbmSendLocation" table="tbm_sendlocation">
        <unique-constraints>
            <unique-constraint name="id_UNIQUE" columns="id"/>
        </unique-constraints>
        <field name="chat_id" type="string" column="chat_id" length="45" nullable="false">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="disable_notification" type="boolean" column="disable_notification" nullable="true"/>
        <field name="reply_to_message_id" type="integer" column="reply_to_message_id" nullable="true">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="reply_markup" type="string" column="reply_markup" length="45" nullable="true">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="live_period" type="integer" column="live_period" nullable="true">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="latitude" type="float" column="latitude" nullable="false">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="longitude" type="float" column="longitude" nullable="false">
            <options>
                <option name="fixed"/>
            </options>
        </field>
    </entity>
</doctrine-mapping>