<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TbmAudio" table="tbm_audio">
    <unique-constraints>
      <unique-constraint name="file_id_UNIQUE" columns="file_id"/>
    </unique-constraints>
    <id name="fileId" type="integer" column="file_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="duration" type="integer" column="duration" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="performer" type="string" column="performer" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mimeType" type="string" column="mime_type" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fileSize" type="integer" column="file_size" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
