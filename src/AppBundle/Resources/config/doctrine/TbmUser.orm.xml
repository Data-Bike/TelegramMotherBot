<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TbmUser" table="tbm_user">
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
    </unique-constraints>
    <id name="id" association-key="true"/>
    <field name="firstName" type="string" column="first_name" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastName" type="string" column="last_name" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="languageCode" type="string" column="language_code" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="username" type="string" column="username" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-one field="id" target-entity="TbmMessage" fetch="LAZY">
      <join-columns>
        <join-column name="id" referenced-column-name="from"/>
      </join-columns>
    </one-to-one>
    <many-to-many field="messageMessage" target-entity="TbmMessage" mapped-by="user" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
