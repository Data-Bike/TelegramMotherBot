<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TbmDocument" table="tbm_document">
    <indexes>
      <index name="thumb" columns="thumb"/>
    </indexes>
    <id name="fileId" type="integer" column="file_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="thumb" type="integer" column="thumb" nullable="true">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="fileName" type="string" column="file_name" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mimeType" type="string" column="mime_type" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fileSize" type="string" column="file_size" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
