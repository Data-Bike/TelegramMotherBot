<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TbmVideonote" table="tbm_videonote">
    <indexes>
      <index name="thumb" columns="thumb"/>
    </indexes>
    <id name="file" association-key="true"/>
    <field name="length" type="integer" column="length" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="duration" type="integer" column="duration" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="thumb" type="integer" column="thumb" nullable="true">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="fileSize" type="integer" column="file_size" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <one-to-one field="file" target-entity="TbmMessage" fetch="LAZY">
      <join-columns>
        <join-column name="file_id" referenced-column-name="video_note"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
