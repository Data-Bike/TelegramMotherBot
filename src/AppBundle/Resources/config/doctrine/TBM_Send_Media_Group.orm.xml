<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="AppBundle\Entity\TbmSendMediaGroup" table="tbm_sendmediagroup">
        <indexes>
            <index name="media" columns="media"/>
        </indexes>
        <unique-constraints>
            <unique-constraint name="id_UNIQUE" columns="id"/>
        </unique-constraints>
        <field name="chat_id" type="string" column="chat_id" length="45" nullable="false">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="disable_notification" type="boolean" column="disable_notification" nullable="true"/>
        <field name="reply_to_message_id" type="integer" column="reply_to_message_id" nullable="true">
            <options>
                <option name="fixed"/>
            </options>
        </field>
    </entity>
</doctrine-mapping>