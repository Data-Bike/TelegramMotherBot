<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TbmUserprofilephotos" table="tbm_userprofilephotos">
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="totalCount" type="integer" column="total_count" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-many field="photosizeFile" target-entity="TbmPhotosize" inversed-by="userprofilephotos" fetch="LAZY">
      <join-table name="tbm_userprofilephotos_has_photosize">
        <join-columns>
          <join-column name="UserProfilePhotos_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="PhotoSize_file_id" referenced-column-name="file_id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
