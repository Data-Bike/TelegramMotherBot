<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TbmPhotosize" table="tbm_photosize">
    <unique-constraints>
      <unique-constraint name="file_id_UNIQUE" columns="file_id"/>
    </unique-constraints>
    <id name="file" association-key="true"/>
    <field name="width" type="integer" column="width" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="height" type="integer" column="height" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fileSize" type="integer" column="file_size" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <one-to-one field="file" target-entity="TbmVideonote" fetch="LAZY">
      <join-columns>
        <join-column name="file_id" referenced-column-name="thumb"/>
      </join-columns>
    </one-to-one>
    <many-to-many field="messageMessage" target-entity="TbmMessage" mapped-by="photosizeFile" fetch="LAZY"/>
    <many-to-many field="userprofilephotos" target-entity="TbmUserprofilephotos" mapped-by="photosizeFile" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
